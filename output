from twilio.rest import Client

#define vairables here
#Account SID
account_sid = ""
#Auth token
auth_token  = ""
#Twilio number
twilio_number = ""
#User number
output_number = ""
#Message you want to output through sms
output_message = "test"


client = Client(account_sid, auth_token)

message = client.messages.create(
    to=output_number, 
    from_=twilio_number,
    body=output_message)

print(message.sid)
